{{#each this}}
  <article class="app_cart">
    <div class="head">
      <img src={{app_img}} class="company_icon" alt="" />
      <ul>
        <li class="name">{{title}}</li>
        <li class="status">{{discount}}</li>
      </ul>
    </div>
    <p class="cart_desc">
      {{price}}
    </p>

      <a class="btn" href="">Visit</a>
    </div>
  </article>

{{/each}}